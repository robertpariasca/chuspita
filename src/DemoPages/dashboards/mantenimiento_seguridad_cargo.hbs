{{#> base }}
    <div class="app-main">
        {{> AppSidebar/menubar dash-analytics=this }}
        <div class="app-main__outer">
            <div class="app-main__inner">
                {{> AppHeader/header }}
                <div class="app-inner-layout app-inner-layout-page">
                    
                    <div class="app-inner-layout__wrapper">
                        <div class="app-inner-layout__sidebar">
                            <div class="app-layout__sidebar-inner dropdown-menu-rounded">
                                {{> AppMain/page-menu dash-analytics=this}}
                            </div>
                        </div>
                                            <div class="col-md-6">
                                                <div class="main-card mb-3 card">
                                                    <div class="card-body"><h5 class="card-title">Registro de Cargo</h5>
                                                        <form class="" method="post" id="formsegmancargo">
                                                            <div class="position-relative form-group"><label
                                                                    for="segmancaremp" class="">Empresa</label><select
                                                                    name="select" id="segmancaremp"
                                                                    class="form-control">
                                                                    <option value="000"> </option>
                                                            </select></div>
                                                            <div class="position-relative form-group"><label
                                                                    for="segmancarofi" class="">Sede</label><select
                                                                    name="select" id="segmancarofi"
                                                                    class="form-control">
                                                                    <option value="000"> </option>
                                                            </select></div>
                                                            <div class="position-relative form-group"><label
                                                                    for="segmancardes" class="">Nombre Cargo</label><input
                                                                    name="segmancardes" id="segmancardes"
                                                                    placeholder="Ingrese Nombre Cargo..." type="text"
                                                                    class="form-control" style="text-transform: lowercase"></div>
      
                                                            <button class="mt-1 btn btn-primary" name='agregarCargo' id='agregarCargo'>Agregar</button>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                    </div>
                </div>
            </div>
            <!--{{> AppFooter/footer }}-->
        </div>
    </div>
{{/ base }}
