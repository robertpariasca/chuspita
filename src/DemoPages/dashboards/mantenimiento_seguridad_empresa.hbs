                                                            <div class="position-relative form-group"><label
                                                                    for="segmanemptel" class="">Telefono</label><input
                                                                    name="segmanemptel" id="segmanemptel"
                                                                    placeholder="Ingrese Telefono..." type="text"
                                                                    class="form-control"></div>
      
                                                            <button class="mt-1 btn btn-primary" name='agregarUsuario' id='agregarUsuario'>Agregar</button>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                    </div>
                </div>
            </div>
            <!--{{> AppFooter/footer }}-->
        </div>
    </div>
{{/ base }}
