{{#> base }}
    <div class="app-main">
        {{> AppSidebar/menubar dash-analytics=this }}
        <div class="app-main__outer">
            <div class="app-main__inner">
                {{> AppHeader/header }}
                <div class="app-inner-layout app-inner-layout-page">
                    
                    <div class="app-inner-layout__wrapper">
                        <div class="app-inner-layout__sidebar">
                            <div class="app-layout__sidebar-inner dropdown-menu-rounded">
                                {{> AppMain/page-menu dash-analytics=this}}
                            </div>
                        </div>
                                            <div class="col-md-6">
                                                <div class="main-card mb-3 card">
                                                    <div class="card-body"><h5 class="card-title">Registro de Sede</h5>
                                                        <form class="" method="post" id="formsegmanoficina">
                                                            <div class="position-relative form-group"><label
                                                                    for="segmanofiemp" class="">Empresa</label><select
                                                                    name="select" id="segmanofiemp"
                                                                    class="form-control">
                                                                    <option value="000"> </option>
                                                            </select></div>
                                                            <div class="position-relative form-group"><label
                                                                    for="segmanofides" class="">Descripcion Sede</label><input
                                                                    name="segmanofides" id="segmanofides"
                                                                    placeholder="Ingrese Sede..." type="text"
                                                                    class="form-control"></div>
                                                            <div class="position-relative form-group"><label
                                                                    for="segmanofidir" class="">Direccion</label><input
                                                                    name="segmanofidir" id="segmanofidir"
                                                                    placeholder="Ingrese Direccion..." type="text"
                                                                    class="form-control"></div>
                                                            <div id="btnform">
                                                                    <button class="mt-1 btn btn-primary" name='agregarOficina' id='agregarOficina'>Agregar</button>
                                                            </div>
                                                            
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                    </div>
                </div>
            </div>
            <!--{{> AppFooter/footer }}-->
        </div>
    </div>
{{/ base }}
